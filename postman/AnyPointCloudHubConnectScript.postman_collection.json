	"item": [
		{
			"name": "GetBearerToken",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const options = {\r",
							"    url: pm.environment.get(\"OAuth2_Url\"),\r",
							"    method: 'POST',\r",
							"    header:{\r",
							"        'Accept': '*/*',\r",
							"        'Content-Type': 'application/x-www-form-urlencoded'\r",
							"    },\r",
							"    body: {\r",
							"        mode: 'urlencoded',\r",
							"        urlencoded:[\r",
							"            { key: 'client_id', value: pm.environment.get(\"OAuth2_clientId\")},\r",
							"            { key: 'client_secret', value: pm.environment.get(\"OAuth2_secretId\")},\r",
							"            { key: 'grant_type', value: 'client_credentials'}\r",
							"        ]        \r",
							"    }\r",
							"};\r",
							"\r",
							"pm.sendRequest(options, function (err, res) {\r",
							"    console.log(\"token = \"  + res.json().access_token)\r",
							"    pm.environment.set(\"mule_access_token\", res.json().access_token);\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
